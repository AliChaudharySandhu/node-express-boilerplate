# top-most EditorConfig file
root = true

# Windows-style newlines with a newline ending every file
[*]
end_of_line = crlf
insert_final_newline = true

# Set default charset
charset = utf-8

# General identation style
[*]
indent_style = tab
indent_size = 2

[*.yaml]
indent_syle = space
indent_size = 2
